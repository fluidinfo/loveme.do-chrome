{
  "name": "Fluidinfo",
  "version": "2.0.4",
  "description": "Talk about the page you're looking at or the text you have selected in a sidebar.",
  "omnibox": { "keyword" : "fi" },
  "background_page": "background.html",
  "options_page": "fancy-settings/settings.html",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "icons" : {
    "16" : "images/fi-16.png",
    "19" : "images/fi-19.png",
    "32" : "images/fi-32.png",
    "48" : "images/fi-48.png",
    "64" : "images/fi-64.png",
    "128" : "images/fi-128.png"
  },
  "browser_action": {
    "default_icon": "images/fi-19.png",
    "default_title": "Fluidinfo"
  },
  "content_scripts": [
    {
      "css": ["sidebar.css"],
      "js": ["shortcut.js", "jquery-1.7.1.min.js", "global.js", "values.js", "sidebar.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    },
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
