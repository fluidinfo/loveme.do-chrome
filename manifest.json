{
  "name": "Fluidinfo Thing Engine",
  "version": "1.0",
  "description": "The Fluidinfo Thing Engine extension",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background_page": "background.html",
  "permissions": ["contextMenus", "tabs", "http://*/*"],
  "icons" : {
    "16" : "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
