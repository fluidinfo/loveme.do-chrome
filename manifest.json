{
  "name": "Fluidinfo",
  "version": "1.1.2",
  "description": "A chrome extension to jump to Fluidinfo to see & tag browser URLs and other selected text.",
  "background_page": "background.html",
  "permissions": ["contextMenus", "tabs"],
  "icons" : {
    "16" : "fi_16.png",
    "48" : "fi_48.png",
    "64" : "fi_64.png",
    "128" : "fi_128_96.png"
  },
  "page_action": {
    "default_icon": "fi_16.png",
    "default_title": "See this page in Fluidinfo"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
