{
  "name": "Fluidinfo",
  "version": "1.1.5",
  "description": "A chrome extension to jump to Fluidinfo to see & tag browser URLs and other selected text.",
  "background_page": "background.html",
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs",
    "unlimitedStorage"
  ],
  "icons" : {
    "16" : "fi_16.png",
    "48" : "fi_48.png",
    "64" : "fi_64.png",
    "128" : "fi_128.png"
  },
  "browser_action": {
    "default_icon": "fi_19.png",
    "default_title": "Fluidinfo"
  },
  "content_scripts": [
    {
      "css": ["tag.css"],
      "js": ["jquery-1.7.1.min.js", "content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
