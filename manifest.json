{
  "name": "Fluidinfo",
  "version": "1.1",
  "description": "A chrome extension to jump to Fluidinfo to see & tag browser URLs and selected text.",
  "background_page": "background.html",
  "permissions": ["contextMenus", "tabs"],
  "icons" : {
    "16" : "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "See this page in Fluidinfo"
  }
}
