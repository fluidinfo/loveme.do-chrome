{
  "name": "Fluidinfo",
  "version": "1.5.38",
  "description": "Add your info to URLs, and jump to Fluidinfo pages for them and for selected text.",
  "omnibox": { "keyword" : "fi" },
  "background_page": "background.html",
  "options_page": "fancy-settings/settings.html",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "icons" : {
    "16" : "fi_16.png",
    "19" : "fi_19.png",
    "32" : "fi_32.png",
    "48" : "fi_48.png",
    "64" : "fi_64.png",
    "128" : "fi_128.png"
  },
  "browser_action": {
    "default_icon": "fi_19.png",
    "default_title": "Fluidinfo"
  },
  "content_scripts": [
    {
      "css": ["sidebar.css"],
      "js": ["shortcut.js", "jquery-1.7.1.min.js", "sidebar.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    },
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
